{
    "env": {
        "browser": true,
        "es2020": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        // "no-invalid-this": 2,
        "no-extra-semi": "error",
        "no-extra-boolean-cast": "error",
        "no-undef": "error"
    },
    "globals": {
        "gdi": "readonly",
        "plman": "readonly",
        "window": "readonly",
        "include": "readonly",
        "utils": "readonly",
        "on_paint": "writable"
    }
}